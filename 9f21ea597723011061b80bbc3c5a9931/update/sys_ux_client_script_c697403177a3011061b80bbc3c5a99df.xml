        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function handler({api, event, helpers, imports}) {
    var arr = [];
    var results = api.data.verticalC_headlinemetric1.output.result;
    var clientStateParameter = 'verticalCMetric1ShowHealth';

    for (let i = 0; i < results.length; i++) {
        var value = results[i].value;
        if (value<7 && value>0) {
            arr.push('green');
        } else if (value>=7 && value<=30) {
            arr.push('amber');
        } else if (value>30){
            arr.push('red');
        } else {
            arr.push('');
        }
    }

    api.setState(clientStateParameter, arr);
}]]></script>
