        <name>showOverallHealth</name>
        <preset/>
        <required_translations>[
    
]</required_translations>
        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function mostFrequentColor({api, colors = [], clientStateParameter = 'ElementOverallHealth'}) {
    
    var colors = api.state.ElementHealthList;
    let frequency = {'green': 0, 'amber': 0, 'red': 0};
    
    // Count the frequencies of the colors
    colors.forEach(color => {
        if (color) {
            frequency[color]++;
        }
    });

    // Determine the most frequent color
    let mostFrequent = 'amber'; // Default to 'amber' if frequencies are equal or all are zero
    let maxCount = frequency['amber'];

    for (let color in frequency) {
        if (frequency[color] > maxCount) {
            mostFrequent = color;
            maxCount = frequency[color];
        }
    }
    
    api.setState(clientStateParameter, mostFrequent);
}]]></script>
