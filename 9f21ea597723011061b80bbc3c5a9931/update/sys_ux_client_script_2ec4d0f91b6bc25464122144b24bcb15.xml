        <script><![CDATA[/**
 * @param {params} params
 * @param {api} params.api
 * @param {TransformApiHelpers} params.helpers
 */
function evaluateProperty({
    api,
    helpers
}) {
    /* The code below checks whether there the length of the JSON payload from the API
    is the same as the row for which the link is implemented. If there is dictionary
    in the payload for the corrsponding link then the link is hidden. Additionally,
    if there is a dictionary in the payload but the value is not >=0 and not null
    then the link is also hidden 
    */
    var i=4;
    var result = api.data.verticalC_headlinemetric2.output.result;
    if (result.length >= i+1) {
        var value = result[i].value;
        return !(value >= 0 && value !== null);
    }
    return true;
}
]]></script>
