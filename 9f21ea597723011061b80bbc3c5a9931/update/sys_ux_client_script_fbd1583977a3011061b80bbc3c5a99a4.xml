        <name>showVerticalCMetric2Health</name>
        <preset/>
        <required_translations>[
    
]</required_translations>
        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function handler({api, event, helpers, imports}) {
    var arr = [];
    var results = api.data.verticalC_headlinemetric2.output.result;
    var clientStateParameter = 'verticalCMetric2ShowHealth';
    var m = api.state.Month;

    for (let i = 0; i < results.length; i++) {
        var value = results[i].scores[m].value;
        if (value>20) {
            arr.push('green');
        } else if (value>=10 && value<=20) {
            arr.push('amber');
        } else if (value<10 && value>=0 && value !== null){
            arr.push('red');
        } else {
            arr.push('');
        }
    }

    api.setState(clientStateParameter, arr);
}]]></script>
