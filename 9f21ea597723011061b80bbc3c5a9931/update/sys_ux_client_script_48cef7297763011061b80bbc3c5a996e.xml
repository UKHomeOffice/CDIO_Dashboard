        <includes/>
        <macroponent display_value="HO Product Performance League Table default">278faed57763011061b80bbc3c5a99c6</macroponent>
        <name>showVerticalAMetric1Health</name>
        <preset/>
        <required_translations>[ ]</required_translations>
        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function handler({api, event, helpers, imports}) {
    var value = api.data.verticalA_headlinemetric1.output.result[0].scores[0].value;
    var clientStateParameter = 'verticalAMetric1ShowHealth';

    if(value<13 && value>0){
        api.setState(clientStateParameter,'green');
    }
    else if(value>=13 && value<20){
        api.setState(clientStateParameter,'amber');
    }
    else if (value>20) {
        api.setState(clientStateParameter,'red');
    }
    else {
        api.setState(clientStateParameter,'');
    }

}]]></script>
