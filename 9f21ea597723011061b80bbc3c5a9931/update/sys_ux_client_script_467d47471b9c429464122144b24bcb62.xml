function evaluateProperty({api, helpers}) {
	const input = {
  "key1": [{ "green": 2 }],
  "key2": [{ "amber": 1, "red": 1 }],
  "key3": [{ "red": 2 }],
  'key4': [{ 'red':3}]
};

const sortedKeys = Object.keys(input).sort((a, b) => {
  const countA = input[a][0]["red"] || 0;
  const countB = input[b][0]["red"] || 0;
  return countB - countA;
});

const sortedInput = {};
sortedKeys.forEach(key => {
  sortedInput[key] = input[key];
});

