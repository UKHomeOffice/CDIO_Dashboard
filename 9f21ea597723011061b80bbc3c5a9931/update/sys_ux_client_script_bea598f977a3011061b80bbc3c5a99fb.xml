        <name>showVerticalAMetric2Health</name>
        <preset/>
        <required_translations>[
    
]</required_translations>
        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function handler({api, event, helpers, imports}) {
    var arr = [];
    var results = api.data.verticalA_headlinemetric2.output.result;
    var clientStateParameter = 'verticalAMetric2ShowHealth';

    for (let i = 0; i < results.length; i++) {
        var value = results[i].value;
        if (value>97.5) {
            arr.push('green');
        } else if (value>=95 && value<=97.5) {
            arr.push('amber');
        } else if (value<95 && value>0){
            arr.push('red');
        } else {
            arr.push('');
        }
    }

    api.setState(clientStateParameter, arr);
}]]></script>
