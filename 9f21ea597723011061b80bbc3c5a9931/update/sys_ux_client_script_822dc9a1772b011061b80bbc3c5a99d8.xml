        <macroponent display_value="HO Product Performance Table default">278faed57763011061b80bbc3c5a99c6</macroponent>
        <name>showVerticalBMetric1Health</name>
        <preset/>
        <required_translations>[ ]</required_translations>
        <script><![CDATA[/**
* @param {params} params
* @param {api} params.api
* @param {any} params.event
* @param {any} params.imports
* @param {ApiHelpers} params.helpers
*/
function handler({api, event, helpers, imports}) {
    var value = api.data.verticalB_headlinemetric1.output.result[0].scores[0].value;
    var clientStateParameter = 'verticalBMetric1ShowHealth';

    if(value>97.5){
        api.setState(clientStateParameter,'green');
    }
    else if (value>=95 && value<=97.5 ){
        api.setState(clientStateParameter,'amber');
    }
    else if (value<95 && value>0 ) {
        api.setState(clientStateParameter,'red');
    }
    else {
        api.setState(clientStateParameter,'');
    }

}]]></script>
