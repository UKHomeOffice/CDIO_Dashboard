        <name>showHealth</name>
        <preset/>
        <required_translations>[ ]</required_translations>
        <script><![CDATA[/**
  * @param {params} params
  * @param {api} params.api
  * @param {TransformApiHelpers} params.helpers
  */
function evaluateProperty({api, helpers}) {
  var VAHealth = api.state.verticalAShowHealth;
  var VBHealth = api.state.verticalBShowHealth;
  var VCHealth = api.state.verticalCShowHealth;
  var VDHealth = api.state.verticalDShowHealth;
  var clientStateParameter = 'ShowHealth';

  
  let arr = [VAHealth, VBHealth, VCHealth, VDHealth];
  const itemCounter = (value, index) => {
    return value.filter((x) => x == index).length;
};
  if (itemCounter(arr, 'green') === 3) {
    api.setState(clientStateParameter, 'green');
  }
  else if (itemCounter(arr, 'red') === 3) {
    api.setState(clientStateParameter, 'red');
  }
  else {
    api.setState(clientStateParameter, 'amber');
  }
}]]></script>
